{
  "name": "nextjs-notion-starter-kit",
  "version": "2.2.0",
  "private": true,
  "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/nextjs-notion-starter-kit",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "@keyvhq/core": "^2.1.1",
    "@keyvhq/redis": "^2.1.4",
    "@vercel/og": "^0.6.2",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.7.2",
    "got": "^14.4.2",
    "isomorphic-unfetch": "^4.0.2",
    "lqip-modern": "^2.1.0",
    "next": "14",
    "notion-client": "^6.16.0",
    "notion-types": "^6.16.0",
    "notion-utils": "^6.16.0",
    "p-map": "^7.0.2",
    "p-memoize": "^7.1.1",
    "posthog-js": "^1.149.2",
    "react": "^18.3.1",
    "react-body-classname": "^1.3.1",
    "react-dom": "^18.3.1",
    "react-notion-x": "^6.16.0",
    "react-icons": "^5.2.1",
    "react-tweet-embed": "^2.0.0",
    "react-use": "^17.5.1",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  }
}